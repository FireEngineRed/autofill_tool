{
  "manifest_version": 2,

  "name": "QA Testing Autofill",
  "description": "Simplifies QA testing of the Fireworks CRM by automatically entering dummy data into the applicable fields.",
  "version": "1.0",
  "background" : {
      "scripts" : ["background.js"]
  },
  "content_security_policy": "default-src 'self'",

  "page_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon38.png"},
    "default_title": "QA Autofill",
    "default_popup": "popup.html"
    
  },

  "permissions": [
    "activeTab",
    "tabs",
    "declarativeContent",
    "storage",
    "http://*/", 
    "https://*/"
  ]


}